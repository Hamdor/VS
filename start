#!/bin/bash

nsport=20000
nsname=localhost

orbd -ORBInitialPort $nsport &
java -jar Verkehrschaos.jar name=Streets nameserverport=$nsport nameserverhost=$nsname &
java -jar VerkehrschaosControl.jar name=Streets nameserverport=$nsport nameserverhost=$nsname &

